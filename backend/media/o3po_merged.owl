      <owl:someValuesFrom rdf:resource="http://html.inf.ufrgs.br/home/pos/nosantos/public_html/o3po.owl#fluid"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <oboI:hasRelatedSynonym xml:lang="en">fluid flow</oboI:hasRelatedSynonym>
  <Comm:definition xml:lang="en">def. a process that has partipant some fluid and the fluid is in continuous movement.</Comm:definition>
  <rdfs:comment xml:lang="en">Definition: The continuous movement characteristic of a liquid or gas. [Definition Source: NCI]</rdfs:comment>
  <rdfs:label xml:lang="en">flow</rdfs:label>
  <rdfs:label xml:lang="pt">fluxo</rdfs:label>
</owl:Class>

<owl:Class rdf:about="http://html.inf.ufrgs.br/home/pos/nosantos/public_html/o3po.owl#ICV">
  <rdfs:subClassOf rdf:resource="http://html.inf.ufrgs.br/home/pos/nosantos/public_html/o3po.owl#valve"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="http://html.inf.ufrgs.br/home/pos/nosantos/public_html/o3po.owl#component_of"/>
      <owl:someValuesFrom rdf:resource="http://html.inf.ufrgs.br/home/pos/nosantos/public_html/o3po.owl#tubing"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/hasQuality"/>
      <owl:someValuesFrom rdf:resource="http://html.inf.ufrgs.br/home/pos/nosantos/public_html/o3po.owl#ICV_annular_pressure"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="https://purl.industrialontologies.org/ontology/core/Core/hasQuality"/>
      <owl:someValuesFrom rdf:resource="http://html.inf.ufrgs.br/home/pos/nosantos/public_html/o3po.owl#ICV_annular_temperature"/>
    </owl:Restriction>
  </rdfs:subClassOf>
