  <core:definition xml:lang="en">b has member part c at some time =Def for some time t (c member part of b at t)</core:definition>
  <core:prefLabel xml:lang="en">has member part at some time</core:prefLabel>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://purl.obolibrary.org/obo/BFO_0000117">
