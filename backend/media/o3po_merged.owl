  <rdfs:comment xml:lang="en">A production treating vessel designed to facilitate the separation of gas, oil, and/or water from a produced fluid stream.</rdfs:comment>
  <rdfs:label xml:lang="pt">separador</rdfs:label>
  <rdfs:label xml:lang="en">separator</rdfs:label>
</owl:Class>

<owl:Class rdf:about="http://html.inf.ufrgs.br/home/pos/nosantos/public_html/o3po.owl#SubseaTree">
