<owl:ObjectProperty rdf:about="http://purl.obolibrary.org/obo/BFO_0000057">
  <rdfs:domain rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
  <rdfs:range>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
