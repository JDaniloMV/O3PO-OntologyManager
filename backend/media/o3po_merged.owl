  <rdfs:label xml:lang="en">permeability (quality)</rdfs:label>
  <rdfs:seeAlso xml:lang="en">https://en.wikipedia.org/wiki/Permeability_(Earth_sciences)</rdfs:seeAlso>
</owl:Class>

<owl:Class rdf:about="http://html.inf.ufrgs.br/home/pos/nosantos/public_html/o3po.owl#Petroleum">
