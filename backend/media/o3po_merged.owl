  <rdfs:label xml:lang="en">natural gas</rdfs:label>
  <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://en.wikipedia.org/wiki/Natural_gas</rdfs:seeAlso>
</owl:Class>

<owl:Class rdf:about="http://html.inf.ufrgs.br/home/pos/nosantos/public_html/o3po.owl#Observation_well">
